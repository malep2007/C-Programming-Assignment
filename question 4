#include <stdio.h>
int *addarrays(int x[], int y[], int SIZE);

main(){
    int x[] = {1,2,3,1,2};
    int y[] = {7,8,9,5,6};
    int i;

    int *z = addarrays(x, y, 5);
    for(i=0;i<5;i++) {         printf("%d \n", z[i]);
    }
}
int *addarrays(int x[], int y[], int length)
{
  int *z;
  int i;
 for(i=0;i<length;i++)
    {
        z[i] = x [i] + y[i];
    }
    return z;
}
 
