E)   #include <stdio.h>
counter = 1;
main()
{
    while(counter <= 100)
    {
        printf("%d\n", counter );
        counter += 3;
    }
    return 0;
}


 F)  for (counter =1; counter < MAXVALUES; counter++)            /* We don't terminate on for loop statement*/
