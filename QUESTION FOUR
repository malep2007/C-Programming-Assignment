#include <stdio.h>
#include <stdlib.h>
int j, n;
int main(){


int *addarrays(int n, int x[], int y[], int z[])
        int i;
		for (i=0; i<n; i++)
		{ z[i] = x[i] + y[i];
					}
		return z;
	}
 
 
 
 
 
