#include <stdio.h>
#include <stdlib.h>
void addarrays(int a[5],int b[5]);
int main()
{
    int a[5]={1,2,3,4,5};
    int b[5]={0,6,7,8,9};
 addarrays(a,b);
 return 0;
}
void addarrays(int a[5],int b[5])
{


 int c[5];
 int i;
 for(i=0;i<=5;i++)
 {

c[i]= a[i]+ b[i];
     printf("The sum is %d\n",c[i]);




 }

    return 0;
}
