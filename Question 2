part 3
wrong code
#include <stdio.h>
#include <stdlib.h>
void print_msg(void);

main(){
 print_msg("this is the message to print");
 /*The print_msg() function is not supposed to take any arguments but the main function calls it with a string argument*/
return 0;
}
void print_msg(void){
puts("this is the message to print");
return 0;
//The return 0 is not suppose to be there
}

correct code
#include <stdio.h>
#include <stdlib.h>
void print_msg(void);

main(){
 print_msg();
return 0;
}
void print_msg(void){
puts("this is the message to print");

}





































