#include<stdio.h>
// alway terminate when u call a function
void print_msg (void);
main()
{
    //the braces should be empty hence should not take any aguments
print_msg();
return 0;
}
    //the header function should be identical at any point when called
void print_msg(void)
{
    puts("this is the message to print");

    return 0;
}
