 #include <stdio.h>
#include <stdlib.h>

//Question four
void addarrays(int x[7],int y[7]);

int main()
{
  int x[7]={11,1,2,3,4,5,6};
  int y[7] ={1,3,4,8,0,9,10};
  addarrays(x,y);
  return 0;
}
void addarrays(int x[7],int y[7]){
int z[7];
int i;
for(i=0;i<=7;i++){
    z[i]=x[i]+y[i];
    printf("Sum is %d\n",z[i]);
}
}







