int *addarrays(int arraya[],int arrayb[],int length)
{
    int*final_array= malloc(lenth*sizeof(int));
    for(int i=0;i<lenth;i++)
    {
    final_array[1]=arraya[1] + arrayb[1];
    }
    return final_array;
}

