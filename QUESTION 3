// source code
#include <stdio.h>
#include <stdlib.h>

int main()
{
int j=1;// definition and initialization of variable j
while(j<=100)
{
printf("%d\n",j);
j+=3;
}
return 0;
}

//debug
for (counter = 1; counter < MAXVALUES; counter++ );/*for statement should not be terminated with
a semicolon since its a block*/
printf("\nCounter = %d", counter);


