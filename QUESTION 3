
    long int digits[50];

    digits[49] = 123.456;




   
    for (x=0; x<100; x++){
        printf(" %d\t",x);
    } /*final value of x is 100*/

    
    for (ctr=2; ctr<10; ctr+=3){
        printf(" %d\t",ctr);
    } /*final value of ctr is 11*/


