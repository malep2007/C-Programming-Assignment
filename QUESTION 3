    /*3
   (a)   long values[50];

   (b)    long values[49] = 123456;
   (c)    x is equal to 100 when the statement is complete

   (d)    The value of ctr is 11 when the statement is completes

   */


#include <stdio.h>


main()
{
    int count = 1;
    while(count<=100)
    {
        printf("%d \n", count);
        count +=3;
    }

    return 0;
}
/* The code under the for loop is always executed once, outside of the loop,
because of the semicolon terminating the for loop*/
